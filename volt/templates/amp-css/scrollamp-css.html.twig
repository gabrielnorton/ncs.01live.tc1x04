#block-volt-main-menu{

  min-height: 60px;
}

#block-volt-main-menu ul :first-child{
 position: relative;
  display: block;
  width: 100%;
  height: auto;
  overflow: visible;
  background-color: #45484f;
}
#block-volt-main-menu ul :first-child a{
	width: 25%;
}

#block-volt-main-menu ul li{
 position: relative;
  display: none;
  width: 100%;
  float: left;
  height: auto;
  overflow: visible;
  background-color: #45484f;
}

#block-volt-main-menu ul li{
  display: none;
}

.no-display{
	display:none;
}

#block-volt-main-menu .content{
    margin: 58px 0 0 0;
    position: relative;
}

#block-volt-main-menu li.menu-item.home-icon{
  background-color: black;
}

#block-volt-main-menu li.menu-item.home-icon:hover{
  background-color: white;
}

#block-volt-main-menu .menu.menu-opened {
    display: block;
}

#block-volt-main-menu a{
	margin: 0px;
	padding: 10px;
}

#block-volt-main-menu li {
  position: relative;
  display: none;
  width: 100%;
  float: left;
  height: auto;
  overflow: visible;
  background-color: #45484f;
}

#block-volt-main-menu li {
  position: relative;
  display: block;
  width: 100%;
  float: left;
  height: auto;
  overflow: visible;
  background-color: #45484f;
}

#block-volt-main-menu li ul{
  display: none;
}

/* expanded button */

#block-volt-main-menu a.reveal-list{
    position: absolute;
    background-image: url(/modules/custom/scroll_menu/images/accrd-down-arrow.png);
    background-repeat: no-repeat;
    background-position: center;
    border-radius: 4px;
    top: 0;
    right: 15px;
    width: 40px;
    height: 38px;
    text-indent: 0px;
    cursor: pointer
}

.expand-down-arrow {
  display: none;
}


#block-volt-main-menu a.reveal-list.expanded{
	background-image: url(/modules/custom/scroll_menu/images/accrd-up-arrow.png);
}

#reveal-menu{
    background-image: url(/core/misc/icons/bebebe/hamburger.svg);
    background-repeat: no-repeat;
    background-position: center center;
    position: absolute;
    background-size: 50%;
    height: 60px;
    width: 4em;
    cursor: pointer;
}

.mobile-expanded{
	display: block;
}

.mobile-collapsed{
	display: none;
}

#block-volt-main-menu .menu-item a.is-active{
	background-color: #ffffff;
}

@media all and (min-width: 461px) and (max-width: 1000px){
	 .position-fixed {
      position: fixed;
      z-index: 1000;
      width: 100%;
      background-color: white;
      box-shadow: -2px 1px 3px 1px rgba(0, 0, 0, 0.3333);
    }

    .position-fixed-admin {
      position: fixed;
      z-index: 1000;
      width: 100%;
      background-color: white;
      box-shadow: -2px 1px 3px 1px rgba(0, 0, 0, 0.3333);
    }

    body.toolbar-vertical.toolbar-tray-open .position-fixed-admin {
      width: initial;
      right: 0;
      left: 15rem;
    }

	#block-volt-main-menu{
	  float: left;
	  width: 100%;
	  z-index: 500;
	}

    #block-volt-main-menu .content{
      width: 50%;
	  position: absolute;
	  z-index: 99;
	  border: 1px solid black;
    }

	#block-volt-main-menu a{
	  margin: 0px;
	  padding: 10px;
	}

	#block-volt-main-menu li {
	  position: relative;
	  display: block;
	  width: 100%;
	  float: left;
	  height: auto;
	  overflow: visible;
	  background-color: #45484f;
	}

	/*arrow pointing to the menu button*/

    #block-volt-main-menu .clearfix.menu:after{
      content: '';
	  position: absolute;
	  left: 16px;
	  top: -10px;
	  border-left: 10px solid transparent;
	  border-right: 10px solid transparent;
	  border-bottom: 10px solid black;
	  z-index: 10000;
	  width: 0;
	  height: 0;
    }
}

	.expand-down-arrow {
	  display: inline;
	}

    .position-fixed {
      position: fixed;
      z-index: 1000;
      left: 0;
      width: 100%;
      background-color: white;
      box-shadow: -2px 1px 3px 1px rgba(0, 0, 0, 0.3333);
    }

    body.toolbar-vertical.toolbar-tray-open .position-fixed-admin {
      width: initial;
      right: 0;
      left: 15rem;
    }

    .position-fixed-admin {
      position: fixed;
      z-index: 1000;
      left: 0;
      width: 100%;
      background-color: white;
      box-shadow: -2px 1px 3px 1px rgba(0, 0, 0, 0.3333);
    }

    #block-volt-main-menu li.menu-item.home-icon{
	  background-color: white;
	}

    #block-volt-main-menu {
      display: block;
      background-color: #FFFFFF;
      padding: 10px 0px;
      max-width: 970px;
      margin: auto;
      border: none;
    }


    #block-volt-main-menu .content{
      margin: 0;
    }


    #block-volt-main-menu ul {
      list-style: none;
      margin: 0;
    }


    #block-volt-main-menu .clearfix.menu{
      text-align: justify;
      height: 44px;
      padding: 0px 15px;
    }

    #block-volt-main-menu .clearfix.menu:after{
      width: 100%;
      content: '';
    }



    #block-volt-main-menu li {
	  position: relative;
	  display: block;
	  float: left;
	  width: auto;
	  background-color: white;
    }

    #block-volt-main-menu li a {
        display: block;
        white-space: nowrap;
        font-weight: normal;
        padding: 10px 0px;
        margin: 0;
        width: 100%;
        height: 100%;
        color:white;
    }

    #block-volt-main-menu li a:hover {
        color: #777;
    }

    #block-volt-main-menu ul li
    {
      height: 44px;
      outline: 0;
      float: none;
    }

    #block-volt-main-menu ul li ul {
      visibility: hidden; /* here */
      float: left;
      position: absolute;
      margin-top: 38px;
      margin-left: 0px;
      left: 0;
      z-index: 999;
    }

    #block-volt-main-menu ul li:last-child ul{
        left: inherit;
        right: 0px;
    }

    #block-volt-main-menu .menu-item > ul{
      background-color: white;
      border: 1px solid black;
      border-radius: 0px 0px 5px 5px;

    }

    #block-volt-main-menu ul li:hover > ul,
    #block-volt-main-menu ul li ul:hover,
    #block-volt-main-menu ul li:hover li.visible {
      visibility: visible;
      display: block;
    }

    #block-volt-main-menu ul li ul li {
      clear: both;
      padding: 0;
      width: 100%;
      float: left;
    }

    .My.Account,
    .Add.Photos,
    .Log.out {
        visibility: hidden;
        background-color: transparent;
    }

    .My.Account.visible,
    .Add.Photos.visible,
    .Log.out.visible {
        background-color: #529600;
    }

    .Login.hide {
        display: none !important;
    }

    .navigation.menu--main.non-mobile-menu ul li ul li {
      clear: both;
      padding:  0; /*5px 0 5px 18px;*/
      width: 100%;
      float: left;
    }

    /* the arrow on top of submenus*/

    #block-volt-main-menu .menu li:last-child ul.menu:after{
      content: '';
      position: absolute;
      right: 10px;
      top: -5px;
      border-left: 5px solid transparent;
      border-right: 5px solid transparent;
      border-bottom: 5px solid black;
      z-index: 10000;
      width: 0;
      height: 0;
    }

    #block-volt-main-menu .menu li:not(:last-child) > ul.menu:after{
      content: '';
      position: absolute;
      left: 10px;
      top: -5px;
      border-left: 5px solid transparent;
      border-right: 5px solid transparent;
      border-bottom: 5px solid black;
      z-index: 10000;
      width: 0;
      height: 0;
    }

    /* display menu */
    #block-volt-main-menu a.reveal-list{
	    display: none;
    }

    #reveal-menu{
        display: none;
    }

    /*override mobile collapsed*/
    .mobile-collapsed{
        display:block;
    }
